{
  "name": "@sentry-internal/node-integration-tests",
  "version": "8.36.0",
  "license": "MIT",
  "engines": {
    "node": ">=14.18"
  },
  "private": true,
  "main": "build/cjs/index.js",
  "module": "build/esm/index.js",
  "types": "build/types/src/index.d.ts",
  "scripts": {
    "build": "run-s build:transpile build:types",
    "build:dev": "yarn build",
    "build:transpile": "rollup -c rollup.npm.config.mjs",
    "build:types": "tsc -p tsconfig.types.json",
    "clean": "rimraf -g **/node_modules && run-p clean:script",
    "clean:script": "node scripts/clean.js",
    "prisma:init": "(cd suites/tracing/prisma-orm && ts-node ./setup.ts)",
    "lint": "eslint . --format stylish",
    "fix": "eslint . --format stylish --fix",
    "type-check": "tsc",
    "pretest": "run-s --silent prisma:init",
    "test": "jest --config ./jest.config.js",
    "test:watch": "yarn test --watch"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.552.0",
    "@hapi/hapi": "^21.3.10",
    "@nestjs/common": "^10.3.7",
    "@nestjs/core": "^10.3.3",
    "@nestjs/platform-express": "^10.4.6",
    "@prisma/client": "5.9.1",
    "@sentry/aws-serverless": "8.36.0",
    "@sentry/node": "8.36.0",
    "@sentry/types": "8.36.0",
    "@sentry/utils": "8.36.0",
    "@types/mongodb": "^3.6.20",
    "@types/mysql": "^2.15.21",
    "@types/pg": "^8.6.5",
    "amqplib": "^0.10.4",
    "apollo-server": "^3.11.1",
    "axios": "^1.7.7",
    "connect": "^3.7.0",
    "cors": "^2.8.5",
    "cron": "^3.1.6",
    "dataloader": "2.2.2",
    "express": "^4.21.1",
    "generic-pool": "^3.9.0",
    "graphql": "^16.3.0",
    "http-terminator": "^3.2.0",
    "ioredis": "^5.4.1",
    "kafkajs": "2.2.4",
    "lru-memoizer": "2.3.0",
    "mongodb": "^3.7.3",
    "mongodb-memory-server-global": "^7.6.3",
    "mongoose": "^5.13.22",
    "mysql": "^2.18.1",
    "mysql2": "^3.11.3",
    "nock": "^13.5.5",
    "node-cron": "^3.0.3",
    "node-schedule": "^2.1.1",
    "pg": "^8.7.3",
    "proxy": "^2.1.1",
    "redis-4": "npm:redis@^4.6.14",
    "reflect-metadata": "0.2.1",
    "rxjs": "^7.8.1",
    "yargs": "^16.2.0"
  },
  "devDependencies": {
    "@types/amqplib": "^0.10.5",
    "@types/node-cron": "^3.0.11",
    "@types/node-schedule": "^2.1.7",
    "globby": "11"
  },
  "config": {
    "mongodbMemoryServer": {
      "preferGlobalPath": true,
      "runtimeDownload": false
    }
  },
  "volta": {
    "extends": "../../package.json"
  }
}
