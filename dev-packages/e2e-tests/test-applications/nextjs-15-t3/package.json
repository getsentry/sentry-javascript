    "@trpc/server": "~11.8.0",
    "next": "^15.5.9",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "server-only": "^0.0.1",
    "superjson": "^2.2.1",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@playwright/test": "~1.56.0",
