{
  "name": "nuxt-3-min",
  "description": "E2E test app for the minimum Nuxt 3 version our Nuxt SDK supports.",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "start": "node .output/server/index.mjs",
    "start:import": "node --import ./.output/server/sentry.server.config.mjs .output/server/index.mjs",
    "clean": "npx nuxi cleanup",
    "test": "playwright test -g 'distributed tracing'",
    "test:build": "pnpm install && pnpm build",
    "test:assert": "pnpm test"
  },
  "dependencies": {
    "@sentry/nuxt": "latest || *",
    "nuxt": "3.9.0",
    "vue": "latest",
    "vue-router": "latest"
  },
  "devDependencies": {
    "@playwright/test": "~1.53.2",
    "@sentry-internal/test-utils": "link:../../../test-utils"
  },
  "pnpm": {
    "overrides": {
      "nitropack": "2.10.0",
      "ofetch": "1.4.0",
      "@vercel/nft": "0.29.4"
    }
},
  "volta": {
    "extends": "../../package.json"
  }
}
