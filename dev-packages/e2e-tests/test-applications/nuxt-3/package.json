{
  "name": "nuxt-3",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build && bash ./copyIITM.bash",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "start": "node .output/server/index.mjs",
    "start:import": "node --import ./.output/server/sentry.server.config.mjs .output/server/index.mjs",
    "clean": "npx nuxi cleanup",
    "test": "playwright test",
    "test:build": "pnpm install && pnpm build",
    "test:build-nightly": "pnpm install && pnpm rm nuxt && pnpm add nuxt-nightly@3x && pnpm build",
    "test:assert": "pnpm test"
  },
  "dependencies": {
    "@sentry/nuxt": "latest || *",
    "nuxt": "^3.14.0"
  },
  "devDependencies": {
    "@playwright/test": "~1.50.0",
    "@sentry-internal/test-utils": "link:../../../test-utils"
  },
  "sentryTest": {
    "optionalVariants": [
      {
        "build-command": "test:build-nightly",
        "label": "nuxt-3 (nightly)"
      }
    ]
  }
}
