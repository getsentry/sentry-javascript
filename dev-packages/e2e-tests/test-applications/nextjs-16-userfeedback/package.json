  },
  "dependencies": {
    "@sentry/nextjs": "latest || *",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "next": "^16",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "typescript": "~5.0.0"
  },
  "devDependencies": {
    "@playwright/test": "~1.56.0",
    "@sentry-internal/test-utils": "link:../../../test-utils",
    "@tailwindcss/postcss": "^4",
    "tailwindcss": "^4"
  },
  "volta": {
    "extends": "../../package.json"
  }
}
