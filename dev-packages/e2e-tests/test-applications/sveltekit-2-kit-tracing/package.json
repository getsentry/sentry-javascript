    "@sveltejs/kit": "2.49.5",
    "@sveltejs/vite-plugin-svelte": "^6.1.3",
    "svelte": "^5.38.3",
    "svelte-check": "^4.3.1",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "vite": "^7.1.3"
