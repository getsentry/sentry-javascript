    "@types/react-dom": "18.0.11",
    "concurrently": "7.6.0",
    "next": "14.2.25",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "supabase": "2.19.7",
    "typescript": "4.9.5"
  },
  "devDependencies": {
    "@playwright/test": "~1.50.0",
    "@sentry-internal/test-utils": "link:../../../test-utils",
