{
  "compilerOptions": {
    "declaration": true,
    "downlevelIteration": true,
    "inlineSources": true,
    "lib": ["es2015"],
    "module": "commonjs",
    "moduleResolution": "node",
    "noFallthroughCasesInSwitch": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "pretty": true,
    "sourceMap": true,
    "strict": true,
    "target": "es5"
  }
}
